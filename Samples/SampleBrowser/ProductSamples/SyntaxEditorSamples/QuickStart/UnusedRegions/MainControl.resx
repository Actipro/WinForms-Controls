	/// &lt;summary&gt;
	/// This method declares variables that are never referenced.
	/// &lt;/summary&gt;
	public void TestMethod() {
		int unusedVariable;
		int anotherUnusedVariable;
		
		Debug.WriteLine(DateTime.Now + ": Unused region visualization is great!");
		
